@page "/events/{eventId}"
@using DotNetSide.Model
@using System.Threading.Tasks
@inject EventService service

<div class="row">
    <hr/>
</div>
<EventDetail evento="@evento" />

@functions
{
    [Parameter] string eventId {get;set;}
    
    Event evento;

    protected override async Task OnParametersSetAsync()
    {
        evento = await service.GetEvent(eventId);
    }
}